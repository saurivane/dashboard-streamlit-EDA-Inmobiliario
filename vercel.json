{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "installCommand": "pip install -r requirements.txt",
  "framework": "streamlit",
  "devCommand": "streamlit run app.py --server.port=$PORT",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
